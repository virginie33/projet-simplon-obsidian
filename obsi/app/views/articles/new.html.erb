<%= form_for :article, url: articles_path do |f| %>
 
 <%= link_to 'New article', new_article_path %>
<%= link_to 'Back', articles_path %>
 
 
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
